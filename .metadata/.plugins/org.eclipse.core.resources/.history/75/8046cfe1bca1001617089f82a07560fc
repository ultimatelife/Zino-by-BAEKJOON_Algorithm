#include <iostream>
#include <algorithm>
#include <vector>
#include <deque>
using namespace std;

int len = 0;
vector<int> arr;
int qNum = 0;
vector<pair<int, int>> qList;

deque<int> save;

int check[1000001][1000001] = {{-1,}};

void makeDisable(int x, int y){
	if(x >= len || y >= len || x < 0 || y < 0 || check[x][y] != -1){
		return;
	}
	check[x][y] = 0;
	makeDisable(--x, ++y);
}

void checkP(int x, int y){
	while(x < y){
		if(check[x][y] == 0){
			makeDisable(x + 1, y + 1);
			break;
		}

		if(arr[x] != arr[y]){
			makeDisable(x, y);
			break;
		}
		x++,y--;
	}

	save.push_back(check[x][y]);
}

int main() {
	cin >> len;
	for(int i = 0 ; i < len ; i++){
		int temp;	cin >> temp;
		arr.push_back(temp);
	}



	cin >> qNum;
	for(int k = 0 ; k < qNum ; k++){
		int x,y;	cin >> x >> y;
		checkP(x, y);
	}

	while(!save.empty()){
		cout << save.
	}

	return 0;
}


