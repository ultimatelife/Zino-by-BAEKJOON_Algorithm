#include <iostream>
#include <vector>
#include <algorithm>

class Info{
public:
	int node;
	int cost;

	Info(int n, int c){
		node = n;
		cost = c;
	}
};

using namespace std;

int V, E;
vector<Info> edge[200001];

int main() {
	scanf("%d %d", &V, &E);
	for(int i = 1 ; i <= E; i++){
		int a,b,c;
		scanf("%d %d %d", &a, &b, &c);
		edge[a].push_back(Info(b, c));
		edge[b].push_back(Info(a, c));
	}


	return 0;
}
