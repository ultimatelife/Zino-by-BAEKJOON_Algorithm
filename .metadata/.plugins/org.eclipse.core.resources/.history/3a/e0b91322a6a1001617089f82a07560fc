#include <iostream>
#include <algorithm>
#include <vector>
using namespace std;

int main() {
	vector<int> arr;
	arr.assign(10, 0);

	for(int show =  0 ; show < arr.size() ; show++)
		cout << arr[show] << endl;


	return 0;
}


