#include <iostream>
#include <algorithm>
#include <vector>
using namespace std;

int main() {
	vector<int> arr;
	arr.assign(10, 0);

	for(vector<int>::iterator i =  arr.begin() ; i != arr.end() ; i++){
		cout << arr[i] << endl;
	}


	return 0;
}


